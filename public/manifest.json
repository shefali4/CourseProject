{
  "name": "Page Mark",
  "description": "Page Rank on Bookmarks",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Page Marker"
  },
  "permissions" : [
    "activeTab",
    "contextMenus",
    "scripting"
  ],
  "optional_permissions": [
    "<all_urls>"
  ]
}